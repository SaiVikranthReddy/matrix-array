#include<iostream>
using namespace std;
int main()
{
    int a[5][5],m,n,i,j;
    cout<<"Enter the size of rows and columns of matrix"<<endl;
    cin>>m>>n;
    cout<<"Enter the elements of matrix"<<endl;
    for(i=0;i<m;i++)
    for(j=0;j<n;j++)
    cin>>a[i][j];
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        cout<<a[i][j];
        cout<<endl;
    }
}
